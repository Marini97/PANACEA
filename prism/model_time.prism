	[startgetFiles] sched=1 & time1<0 & !progressgetFiles & !DataExfiltration=1 & AccesstoSensitiveFiles=0 & (MisconfiguredApache=1 & AccesstoReverseShell=1) -> (sched'=2) & (time1'=2) & (progressgetFiles'=true);
