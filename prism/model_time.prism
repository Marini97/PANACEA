	[startgetLoginData] sched=1 & time1<0 & !progressgetLoginData & !DataExfiltration=1 & AccesstoMySQL=0 & (AccesstoSensitiveFiles=1) -> (sched'=2) & (time1'=2) & (progressgetLoginData'=true);
